<div class="bigTitle">Language Learning Commons</div>
  <br>
<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <div><label for="user_email" class="languageList">Email</label>
  <%= f.email_field :email, :autofocus => true %></div>

  <div><label for="user_password" class="languageList">Password</label>
  <%= f.password_field :password %></div>

  <% if devise_mapping.rememberable? -%>
    <!-- <div>
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div> -->
  <% end -%>

  <div><%= f.submit "Sign in", :class  => "btn-large" %></div>
<% end %>

<%= render "devise/shared/links" %>